{
	"compilerOptions": {
		"baseUrl": ".",
		"composite": true,
		"declaration": true,
		"incremental": true,
		"isolatedModules": true,
		"jsx": "react",
		"jsxFactory": "h",
		"jsxFragmentFactory": "Fragment",
		"lib": ["esnext", "dom"],
		"module": "esnext",
		"moduleResolution": "node",
		"noUncheckedIndexedAccess": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"skipLibCheck": true,
		"strict": true,
		"target": "esnext"
	}
}
